extends templates/main
block variables
	- let pageTitle = 'Liarch - Architecture & Interior HTML Template | Home Visual';
	- let isHome = true; //- Если true то логотип не будет ссылкой
	- let isDark = false;
	- let currentPage = 1.06; // Для каждой страницы уникален. Номер смотреть в файле pug/templates/_vars.pug в массивах с пунктами меню
	- let currentSection = 1; // // Для каждой раздела уникален. Home -1, about - 2, works - 3, news - 4, pages - 5

	- let bodyModClass = ' webpage--beige';

block header
	include modules/header-4
	include modules/menu
block content
	section.an-awards.an-awards--different(data-aos="fade", data-aos-delay="350")
		.container
			.row
				.an-awards__intro.col-12.col-md.col-lg-6.order-md-1
					.an-awards__image.an-awards__image--mb-negative
						img(src="img/an-awards-image-3.png", width="549", height="832", alt="")
				.an-awards__content.col-12.col-md.col-lg-6
					h1.an-awards__heading.heading.heading--h1 liarch Architects studio
					.an-awards__text We’re from Poland, we passioned with the space & create the values vsustainable over time
					.an-awards__buttons
						a.an-awards__btn.btn(href= URLS.about.default) Learn More
						.video-btn.video-btn--outline
							a.video-btn__btn.fancybox(href="#ourStory")
								span.visually-hidden Play
								+icon('play')(width="20", height="20")
							span.video-btn__text view our story

	.bg-wrapper
		section.simplicity
			.container
				.row
					.col-12.col-md-6.type-service.type-service--lite
						ul.type-service__list
							li.type-service__item(data-aos="fade")
								a(href= URLS.pages.serviceDetail)
									.type-service__item-icon.type-service__item-icon--interior
										+icon('ruler-pencil')(width="49", height="49")
									h3.type-service__item-heading Arch & interior
									.type-service__item-text We bring the magic in to Building & Interior
									.type-service__item-link 250 projects

							li.type-service__item(data-aos="fade", data-aos-delay="300")
								a(href= URLS.pages.serviceDetail)
									.type-service__item-icon.type-service__item-icon--design
										+icon('vector')(width="49", height="49")
									h3.type-service__item-heading Design
									.type-service__item-text Creative design with digital products
									.type-service__item-link 80 projects

							li.type-service__item(data-aos="fade", data-aos-delay="600")
								a(href= URLS.pages.serviceDetail)
									.type-service__item-icon.type-service__item-icon--furniture
										+icon('package')(width="49", height="49")
									h3.type-service__item-heading Products
									.type-service__item-text We create special products and selling it
									.type-service__item-link 350 Products

					.simplicity__content.col-12.col-md-6.col-xl-5.offset-xl-1
						h2.simplicity__heading.heading let us help building your dream
						.simplicity__subheading We know that good design mean that good business.
						.simplicity__text We help our client suceed by creating identities, digital experiences, and printmaterials that communicate clearly, achieve marketing goals & look fantastic Lorem uispum dolor sit amert tunoer poea opefse ceefo goipully.

						.simplicity__notice If you're hurry, quick call for us.
						.simplicity__phone.phone-block.phone-block--theme-beige
							.phone-block__icon
								+icon('phone')(width="18", height="18")
							.phone-block__content
								.phone-block__hint hotline 24/7
								a.phone-block__number(href="tel:+6530025656376") +65 - 3002 56 56 376
				.simplicity__statistics.statistics.row(data-aos="fade")
					.statistics__item.col-12.col-sm
						.statistics__item-value <span class="__js_number" data-end-value="25">0</span>
						.statistics__item-text years of the <br>experience
					.statistics__item.col-12.col-sm
						.statistics__item-value
							span.__js_number(data-end-value="250") 0
						.statistics__item-text satisfitation <br>clients
					.statistics__item.col-12.col-lg
						.statistics__item-value
							span.__js_number(data-end-value="1875") 0
						.statistics__item-text projects completed <br>on 60 countries

	section.works-section
		.works-section__inner.container
			.row
				.works-section__column.col-12.col-md-7
					h2.works-section__heading.heading Works
					.works-section__card.works-section__card--big
						.works-section__card-image
							img(src="img/works-section/01.jpg", width="600", height="600", alt="")
						.works-section__card-tags Residential, Design
						h3.works-section__card-heading
							a(href= URLS.works.singleStandart) Cube villa, Poland
						.works-section__card-text Discover how we helped CleanCloud to increase their presence on a global market by their scaling up its visual identity and building a flexible style guide for their future product elaboration.
					a.works-section__btn.btn(href= URLS.works.grid) more projects

				.works-section__column.works-section__column--list.col-12.col-md-5
					.works-section__card
						.works-section__card-image
							img(src="img/works-section/02.jpg", width="400", height="400", alt="")
						.works-section__card-tags Architecture, CGI
						h3.works-section__card-heading
							a(href= URLS.works.singleStandart) Stone house
						.works-section__card-text Stone House is a digital platform that provides university students with personalized learning pathways

					.works-section__card
						.works-section__card-image
							img(src="img/works-section/03.jpg", width="400", height="400", alt="")
						.works-section__card-tags Architecture, CGI
						h3.works-section__card-heading
							a(href= URLS.works.singleStandart) Single house
						.works-section__card-text Discover how we helped them scale up from local to global.
	section.studio-section
		.studio-section__inner.container
			header.studio-section__header
				h2.studio-section__heading.heading Studio
				.studio-section__text We are surrounded by amazing cultural and physical weather climates. Our HQ is located in Odesa — a beautiful seaside resort city near the Black Sea. Anton Checkhov lived in our same neighborhood, and we are just one minute away from the opera house where Tchaikovsky conducted.
			.studio-section__wrapper
				.studio-section__image.studio-section__image--big
					img(src="img/studio/01.jpg", width="1045", height="640", alt="")
					button.studio-section__play-video.fancybox(href="#ourStory") Play video
				.studio-section__image
					img(src="img/studio/02.jpg", width="470", height="640", alt="")
	section.rewards(data-aos="fade")
		.rewards__inner.container
			ul.rewards__list
				li.rewards__item.reward(data-aos="fade")
					time.reward__date(datetime="2020") 2020
					.reward__logo
						img(src="img/rewards-logo/01-alt.png", alt="")
					.reward__content
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") AAG Winner 2020
								.reward__project-text Canbin Wood In Forest
							a.reward__project-link(href= URLS.works.singleStandart) see project
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") Top 25 Best Architectural In Poland
								.reward__project-text Daniel Villa, Romania
							a.reward__project-link(href= URLS.works.singleStandart) see project
				li.rewards__item.reward(data-aos="fade")
					time.reward__date(datetime="2019") 2019
					.reward__logo
						img(src="img/rewards-logo/02-alt.png", alt="")
					.reward__content
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") The Architect of Season Winner
								.reward__project-text Architect Thiago Alcantara
							a.reward__project-link(href= URLS.works.singleStandart) see project
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") The Green of Arch by AAL US 2019, Slovakia
								.reward__project-text Endora Commercial Center, Germany
							a.reward__project-link(href= URLS.works.singleStandart) see project
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") Art of The Year 2019
								.reward__project-text Reuse Cabins
							a.reward__project-link(href= URLS.works.singleStandart) see project
				li.rewards__item.reward(data-aos="fade")
					time.reward__date(datetime="2018") 2018
					.reward__logo
						img(src="img/rewards-logo/03-alt.png", alt="")
					.reward__content
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") The Trending of Interior 2018 by OKA
								.reward__project-text B65 Apartment, Sepro Building, Italy
							a.reward__project-link(href= URLS.works.singleStandart) see project
	.bg-wrapper
		section.about__our-experts.our-experts.our-experts--mt-negative(data-aos="fade")
			.our-experts__inner.container
				header.our-experts__header
					h2.our-experts__heading.heading our experts
					.our-experts__text At Liarch studio, we’re a group with top architects from building, interior, design, photography, etc. to help consult & build your dream become perfect
				ul.our-experts__list
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-1.png", alt="")
						a.team-item__name(href='#', target='_blank') Stephan Bowie
						.team-item__position CEO Founder
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-2.png", alt="")
						a.team-item__name(href='#', target='_blank') robert downey jr
						.team-item__position CO Founder
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-3.png", alt="")
						a.team-item__name(href='#', target='_blank') laura lorence
						.team-item__position Project Management
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-4.png", alt="")
						a.team-item__name(href='#', target='_blank') david de berg
						.team-item__position Lead of Technical
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-5.png", alt="")
						a.team-item__name(href='#', target='_blank') elena stephan
						.team-item__position Architect
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-6.png", alt="")
						a.team-item__name(href='#', target='_blank') andy robertson
						.team-item__position Architect
					a.our-experts__item.team-item.team-item--join(href='#', target='_blank')
						.team-item__photo
							+icon('users')(width="67", height="45")
							p become <br> our member
				.our-experts__line

		section.pricing.pricing--section
			.container
				h2.pricing__title Pricing & Plan
				.pricing__subtitle At Liarch studio, we’re a group with top architects from building, interior, design, photography, etc.
													|	to help consult & build your dream become perfect
			.container.pricing__container

				.pricing__slider-container
					.pricing__slider.swiper-container.__js_slider-price
						.pricing__wrapper.swiper-wrapper
							.pricing-item.swiper-slide
								.pricing-item__wrapper
									.pricing-item__top
										.pricing-item__plan basic plan
										.pricing-item__caption
									.pricing-item__price
										sup.currency $
										span.digit 49
										span.square / m2
									.pricing-item__tax 10% taxed will include after make price
									ul.pricing-item__included
										li survey & preliminary sketches
										li 3d sketches
									a.pricing-item__btn(href='#') start my project
							.pricing-item.swiper-slide
								.pricing-item__wrapper
									.pricing-item__top
										.pricing-item__plan premium plan
										.pricing-item__caption
											span Popular
									.pricing-item__price
										sup.currency $
										span.digit 69
										span.square / m2
									.pricing-item__tax 10% taxed will include after make price
									ul.pricing-item__included
										li survey & preliminary sketches
										li 3d sketches
										li materials & building
									a.pricing-item__btn(href='#') start my project
							.pricing-item.swiper-slide
								.pricing-item__wrapper
									.pricing-item__top
										.pricing-item__plan ultimate plan
										.pricing-item__caption
									.pricing-item__price
										sup.currency $
										span.digit 99
										span.square / m2
									.pricing-item__tax 10% taxed will include after make price
									ul.pricing-item__included
										li survey & preliminary sketches
										li 3d sketches
										li materials & building
										li free support & repair 2 years
									a.pricing-item__btn(href='#') start my project

	section.our-journal.our-journal--mb
		.container
			header.our-journal__header.our-journal__header--block
				h2.our-journal__heading.heading.heading--upper Our Journal
				.our-journal__text Join into our journal to discuss around art & arch, share to find more inspiration for your work
			ul.our-journal__list.row
				+news-card('01.jpg', '25', 'december', '2020', 'Discover top 15 buildings modern in Italia', 'inspiration', 'By Admin')
				+news-card('02.jpg', '20', 'december', '2020', 'The dome in architectural, the attractive from the art', 'review', 'By Logan Cee')

block footer
	include modules/footer
