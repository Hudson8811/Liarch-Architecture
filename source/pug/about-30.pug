extends templates/main-4
block variables
	- let pageTitle = 'Liarch - Architecture & Interior HTML Template - about visual white';
	- let isHome = false; //- Если true то логотип не будет ссылкой
	- let currentIndex = 3; // Для каждой страницы(раздела) уникален. Номер смотреть в файле pug/templates/_vars.pug в массивах с пунктами меню
	- let bodyModClass = ' webpage--beige';
block header
	include modules/header-4
	include modules/menu
block content
	section.studio-hero(data-aos="fade", data-aos-delay="350")
		.container
			.row.align-items-center
				.studio-hero__intro.col-12.col-md.col-lg-7.order-md-1
					.studio-hero__image
						img(src="img/about/about-30.png", width="804", height="695", alt="")
				.studio-hero__content.col-12.col-md.col-lg-5
					h1.studio-hero__heading.heading bigger, bolder & better
					.studio-hero__text We passioned with the space & create the values vsustainable over time
					.studio-hero__buttons
						.video-btn.video-btn--outline
							a.video-btn__btn.__js_open-modal(href="#ourStory")
								span.visually-hidden Play
								+icon('play')(width="20", height="20")
							span.video-btn__text view our story

	.bg-wrapper
		section.simplicity
			.container
				.row
					.col-12.col-md-6.type-service.type-service--lite
						ul.type-service__list
							li.type-service__item(data-aos="fade")
								.type-service__item-icon.type-service__item-icon--interior
									+icon('ruler-pencil')(width="49", height="49")
								h3.type-service__item-heading
									a(href="#") Arch & interior
								.type-service__item-text We bring the magic in to Building & Interior
								a.type-service__item-link(href="#") 250 projects

							li.type-service__item(data-aos="fade", data-aos-delay="300")
								.type-service__item-icon.type-service__item-icon--design
									+icon('vector')(width="49", height="49")
								h3.type-service__item-heading
									a(href="#") Design
								.type-service__item-text Creative design with digital products
								a.type-service__item-link(href="#") 80 projects

							li.type-service__item(data-aos="fade", data-aos-delay="600")
								.type-service__item-icon.type-service__item-icon--furniture
									+icon('package')(width="49", height="49")
								h3.type-service__item-heading
									a(href="#") Products
								.type-service__item-text We create special products and selling it
								a.type-service__item-link(href="#") 350 Products

					.simplicity__content.col-12.col-md-6.col-xl-5.offset-xl-1
						h2.simplicity__heading.simplicity__heading--upper.heading let us help building your dream
						.simplicity__subheading We know that good design mean that good business.
						.simplicity__text We help our client suceed by creating identities, digital experiences, and printmaterials that communicate clearly, achieve marketing goals & look fantastic Lorem uispum dolor sit amert tunoer poea opefse ceefo goipully.

						.simplicity__notice If you're hurry, quick call for us.
						.simplicity__phone.phone-block.phone-block--theme-beige
							.phone-block__icon
								+icon('phone')(width="18", height="18")
							.phone-block__content
								.phone-block__hint hotline 24/7
								a.phone-block__number(href="tel:+6530025656376") +65 - 3002 56 56 376
				.simplicity__statistics.statistics.row(data-aos="fade")
					.statistics__item.col-12.col-sm
						.statistics__item-value <span class="__js_number" data-end-value="25">0</span>
						.statistics__item-text years of the <br>experience
					.statistics__item.col-12.col-sm
						.statistics__item-value
							span.__js_number(data-end-value="250") 0
						.statistics__item-text satisfitation <br>clients
					.statistics__item.col-12.col-lg
						.statistics__item-value
							span.__js_number(data-end-value="1875") 0
						.statistics__item-text projects completed <br>on 60 countries
	section.approach
		.approach__inner.container
			header.approach__header
				h2.approach__heading.heading Our approach
				.approach__text Progress work flexiable and alway gurantee time-line is our advantage
			.row
				.col-12.col-lg-4
					.approach__card
						.approach__card-icon
							+icon('ticket-alt')(width="48", height="48")
						h3.approach__card-heading Surveys and Quotes
						.approach__card-text Our team will survey your project, collect infor & build quotes
				.col-12.col-lg-4
					.approach__card
						.approach__card-icon
							+icon('bricks')(width="48", height="48")
						h3.approach__card-heading Design and Performance
						.approach__card-text We design best solutions and then performance it based on design
				.col-12.col-lg-4
					.approach__card
						.approach__card-icon
							+icon('customer')(width="48", height="48")
						h3.approach__card-heading Handover and Support
						.approach__card-text After complete project, we will handover full & support 2 years
	.bg-wrapper.bg-wrapper--reverse.bg-wrapper--pb-less
		section.about__our-experts.our-experts(data-aos="fade")
			.our-experts__inner.container
				header.our-experts__header
					h2.our-experts__heading.heading our experts
					.our-experts__text At Liarch studio, we’re a group with top architects from building, interior, design, photography, etc. to help consult & build your dream become perfect
				ul.our-experts__list
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-1.png", alt="")
						a.team-item__name(href='#', target='_blank') Stephan Bowie
						.team-item__position CEO Founder
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-2.png", alt="")
						a.team-item__name(href='#', target='_blank') robert downey jr
						.team-item__position CO Founder
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-3.png", alt="")
						a.team-item__name(href='#', target='_blank') laura lorence
						.team-item__position Project Management
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-4.png", alt="")
						a.team-item__name(href='#', target='_blank') david de berg
						.team-item__position Lead of Technical
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-5.png", alt="")
						a.team-item__name(href='#', target='_blank') elena stephan
						.team-item__position Architect
					.our-experts__item.team-item
						.team-item__photo
							img(src="img/team/item-6.png", alt="")
						a.team-item__name(href='#', target='_blank') andy robertson
						.team-item__position Architect
					a.our-experts__item.team-item.team-item--join(href='#', target='_blank')
						.team-item__photo
							+icon('users')(width="67", height="45")
							p become <br> our member
				.our-experts__line
		.twitter-block(data-aos="fade")
			.container
				h2.twitter-block__heading.heading testimonials
				.about__twitter-block.twitter-block__wrapper
					header.twitter-block__header
						.twitter-block__author
							.twitter-block__author-image
								img(src="img/single-project/avatar.jpg", width="60", height="60", alt="")
							.twitter-block__author-meta
								.twitter-block__author-name Thiago Alcantara
								.twitter-block__author-info <a href="#">@thiago.lfc</a>  - <time datetime="2020-12-15">15 Dec, 2020</time>
						a.twitter-block__social(href="#")
							+icon('twitter')(width="34", height="34")
					.twitter-block__text The team at <a href="#">@liarch.studio</a> is incredibly dedicated, knowledgeable, and helpful. The finished product was beautiful, and worth every penny. I would absolutely recommend Liarch Studio

		section.partners
			.partners__inner.container
				ul.partners__list.partners__list--justify-start
					- let delay = 0;
					each item, index in PARTNERS_LOGO
						- delay = index % 5 === 0 ? 0 : delay + 200;
						if index < 7
							li.partners__item(data-aos="fade", data-aos-delay= delay)
								img(src="img/partners/" + item.name + ".png", width= item.w, height= item.h, alt= item.alt)
	section.rewards(data-aos="fade")
		.rewards__inner.container
			header.rewards__header
				h2.rewards__heading.heading(data-aos="fade") our awards
				.rewards__text This is results for our efforts in many years. Passionate, professional and alway improve better each products
			ul.rewards__list
				li.rewards__item.reward(data-aos="fade")
					time.reward__date(datetime="2020") 2020
					.reward__logo
						img(src="img/rewards-logo/01-alt.png", alt="")
					.reward__content
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") AAG Winner 2020
								.reward__project-text Canbin Wood In Forest
							a.reward__project-link(href="#") see project
						.reward__project
							.reward__project-wrap
								.reward__project-name
									a(href="#") Top 25 Best Architectural In Poland
								.reward__project-text Daniel Villa, Romania
							a.reward__project-link(href="#") see project
				li.rewards__item.reward(data-aos="fade")
					time.reward__date(datetime="2019") 2019
					.reward__logo
						img(src="img/rewards-logo/02-alt.png", alt="")
